{
  "name": "rescript-react-hmr",
  "version": "1.0.0",
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".bs.mjs",
  "bs-dependencies": ["@rescript/react"],
  "warnings": {
    "error": "+A"
  },
  "ppx-flags": [],
  "namespace": true
}
